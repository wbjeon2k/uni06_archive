$(srcdir)/Makefile.in:  Makefile.am  $(top_srcdir)/configure.ac $(ACLOCAL_M4)
	cd $(top_srcdir) && \
	  $(AUTOMAKE) --foreign  ddd/Makefile
Makefile:  $(srcdir)/Makefile.in  $(top_builddir)/config.status
	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@ $(am__depfiles_maybe)

config.h: stamp-h1
	@if test ! -f $@; then \
	  rm -f stamp-h1; \
	  $(MAKE) stamp-h1; \
	else :; fi

stamp-h1: $(srcdir)/config.h.in $(top_builddir)/config.status
	@rm -f stamp-h1
	cd $(top_builddir) && $(SHELL) ./config.status ddd/config.h

distclean-hdr:
	-rm -f config.h stamp-h1
OldMakefile: $(top_builddir)/config.status OldMakefile.in
	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@
vr.h: $(top_builddir)/config.status vr.h.in
	cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@
bld.h: $(top_builddir)/config.status bld.h.in
